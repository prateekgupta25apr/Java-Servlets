<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>Home.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>Mountain Dew</servlet-name>
    <servlet-class>org.prateekgupta.servlets.MountainDewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Mountain Dew</servlet-name>
    <url-pattern>/mountain-dew</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Frooti</servlet-name>
    <servlet-class>org.prateekgupta.servlets.FrootiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Frooti</servlet-name>
    <url-pattern>/frooti</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Pepsi</servlet-name>
    <servlet-class>org.prateekgupta.servlets.PepsiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Pepsi</servlet-name>
    <url-pattern>/pepsi</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Sprite</servlet-name>
    <servlet-class>org.prateekgupta.servlets.SpriteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Sprite</servlet-name>
    <url-pattern>/sprite</url-pattern>
  </servlet-mapping>
</web-app>
